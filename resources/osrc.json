{
    "name": "OSRC Burton Miller",
    "image": "https://bempp.files.wordpress.com/2017/07/hifu.png?w=257&h=214",
    "description": "A OSRC Burton Miller calculation on a provided geometry",
    "script": {
        "run": {
            "source": "osrc.py",
            "destination": "osrc_final.py",
            "patch": true
        }
    },
    "params": [
        {
            "specs": {
                "type": "file"
            },
            "name": "Grid"
        },
        {
            "name": "Properties",
            "params": [
                {
                    "name": "Frequency",
                    "specs": {
                        "type": "slider",
                        "min": "100",
                        "max": "10000000",
                        "default": "3000",
                        "units": "Hz"
                    }
                },
                {
                    "name": "Speed of sound",
                    "specs": {
                        "type": "slider",
                        "min": "10",
                        "max": "3000",
                        "default": "1484",
                        "units": "m/s"
                    }
                },
                {
                    "name": "Incident angle",
                    "specs": {
                        "type": "slider",
                        "min": "0",
                        "max": "360",
                        "default": "30",
                        "step": "1",
                        "units": "deg"
                    }
                },
                {
                    "name": "Solver tolerance",
                    "specs": {
                        "type": "slider",
                        "min": "-100",
                        "max": "-20",
                        "step": "-5",
                        "units": "dB",
                        "default": "-50"
                    }
                },
                {
                    "name": "Number of evaluation points",
                    "specs": {
                        "type": "slider",
                        "min": "1",
                        "max": "1000",
                        "default": "400",
                        "units": "points"
                    }
                }
            ]
        }
    ]
}